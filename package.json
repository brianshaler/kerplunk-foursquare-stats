{
  "name": "kerplunk-foursquare-stats",
  "displayName": "Foursquare Stats",
  "description": "Stats about checkins, venues, categories, etc.",
  "version": "0.0.1",
  "kerplunk": {
    "dependencies": [
      "kerplunk-foursquare"
    ],
    "permissions": {
      "kerplunk-activityitem": {
        "models": [
          "ActivityItem"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Social Networking",
    "kp:Foursquare"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "lodash": "^3.7.0",
    "react": "^0.13.2"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "glut-coffee-amdify": "0.0.0",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp"
  }
}
